<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
	    <Description>
		    Provides integration with Slack.
	    </Description>
	    <PackageTags>elsa extension module slack</PackageTags>
    </PropertyGroup>
    
    <ItemGroup Label="Elsa" Condition="'$(UseProjectReferences)' != 'true'">
      <PackageReference Include="Elsa" />
      <PackageReference Include="SlackNet" />
    </ItemGroup>
    
    <ItemGroup Label="Elsa" Condition="'$(UseProjectReferences)' == 'true'">
      <ProjectReference Include="../../../core/src/Elsa/Elsa.csproj" />
      <PackageReference Include="SlackNet" />
    </ItemGroup>

</Project>
